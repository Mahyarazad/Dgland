    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>40827b5d-24e8-4335-9e66-934b9345af2a</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
